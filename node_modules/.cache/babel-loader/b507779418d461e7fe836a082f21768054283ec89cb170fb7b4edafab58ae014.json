{"ast":null,"code":"import MessageVue from '../Message/Message.vue';\nexport default {\n  name: \"BurguerForm\",\n  data() {\n    return {\n      name: null,\n      bread: null,\n      meat: null,\n      optionals: null,\n      nameBe: null,\n      breadBe: null,\n      meatBe: null,\n      optionalsBe: [],\n      status: \"Solicitado\",\n      msg: null\n    };\n  },\n  methods: {\n    async getIngredients() {\n      const req = await fetch(\"http://localhost:3000/ingredients\");\n      const data = await req.json();\n      this.bread = data.bread;\n      this.meat = data.meat;\n      this.optionals = data.optionals;\n    },\n    async createDelivery(e) {\n      e.preventDefault();\n      const data = {\n        name: this.nameBe,\n        bread: this.breadBe,\n        meat: this.meatBe,\n        optionals: Array.from(this.optionalsBe),\n        status: this.status\n      };\n      const dataJson = JSON.stringify(data);\n      const req = await fetch(\"http://localhost:3000/burgers\", {\n        method: \"POST\",\n        headers: {\n          \"Content-Type\": \"application/json\"\n        },\n        body: dataJson\n      });\n      const res = await req.json();\n      this.nameBe = null, this.breadBe = null, this.meatBe = null, this.optionalsBe = null;\n    }\n  },\n  mounted() {\n    this.getIngredients();\n  },\n  components: {\n    MessageVue\n  }\n};","map":{"version":3,"names":["MessageVue","name","data","bread","meat","optionals","nameBe","breadBe","meatBe","optionalsBe","status","msg","methods","getIngredients","req","fetch","json","createDelivery","e","preventDefault","Array","from","dataJson","JSON","stringify","method","headers","body","res","mounted","components"],"sources":["/home/valentin/Desktop/apps/vue-first-project/src/components/BurguerForm/BurguerForm.vue"],"sourcesContent":["<template>\n    <div id=\"burguerForm\">\n        <form class=\"form\" @submit=\"createDelivery\">\n            <div>\n                <p>Monte seu Hamburguer</p>\n            </div>\n            <div>\n                <label for=\"name\">Nome do cliente:</label>\n                <input type=\"text\" name=\"name\" id=\"name\" v-model=\"nameBe\" placeholder=\"Digite o seu nome\">\n            </div>\n            <div>\n                <label for=\"bread\">Escolha o pão:</label>\n                <select name=\"bread\" id=\"bread\" v-model=\"breadBe\">\n                    <option value=\"\">Selecione seu pão</option>\n                    <option v-for=\"eachOne in bread\" :key=\"eachOne.id\" :value=\"eachOne.name\">{{eachOne.name}}</option>\n                </select>\n            </div>\n            <div>\n                <label for=\"meat\">Escolha a carne:</label>\n                <select name=\"meat\" id=\"meat\" v-model=\"meatBe\">\n                    <option value=\"\">Selecione a carne</option>\n                    <option v-for=\"eachOne in meat\" :key=\"eachOne.id\" :value=\"eachOne.name\">{{eachOne.name}}</option>\n                </select>\n            </div>\n            <div class=\"optionals-container\">\n                <label for=\"optionals\">Escolha os opcionais:</label>\n                <div class=\"checkbox\" v-for=\"eachOne in optionals\" :key=\"eachOne.id\">\n                    <input type=\"checkbox\" name=\"optionals\" id=\"optionals\" v-model=\"optionalsBe\" :value=\"eachOne.name\">\n                    <span>{{eachOne.name}}</span>\n                </div>\n            </div>\n            <div>\n                <input type=\"submit\" class=\"btn-submit\" value=\"Criar meu Burguer\">\n            </div>\n        </form>\n    </div>\n</template>\n<script>\nimport MessageVue from '../Message/Message.vue';\nexport default {\n    name: \"BurguerForm\",\n    data() {\n        return {\n            name: null,\n            bread: null,\n            meat: null,\n            optionals: null,\n            nameBe: null,\n            breadBe: null,\n            meatBe: null,\n            optionalsBe: [],\n            status: \"Solicitado\",\n            msg: null\n        }\n    },\n    methods: {\n        async getIngredients(){\n            const req = await fetch(\"http://localhost:3000/ingredients\");\n            const data = await req.json();\n            \n            this.bread = data.bread;\n            this.meat = data.meat;\n            this.optionals = data.optionals;\n        },\n        async createDelivery(e){\n            e.preventDefault();\n            const data = {\n                name: this.nameBe,\n                bread: this.breadBe,\n                meat: this.meatBe,\n                optionals: Array.from(this.optionalsBe),\n                status: this.status\n            }\n\n            const dataJson = JSON.stringify(data);\n\n            const req = await fetch(\"http://localhost:3000/burgers\",{\n                method: \"POST\",\n                headers: {\"Content-Type\":\"application/json\"},\n                body: dataJson\n            });\n\n            const res = await req.json();\n\n            this.nameBe = null,\n            this.breadBe = null,\n            this.meatBe = null,\n            this.optionalsBe = null\n        }\n    },\n    mounted() {\n        this.getIngredients();\n    },\n    components:{\n        MessageVue\n    }\n}\n</script>\n\n<style scoped>\n    @import \"style.scss\";\n</style>"],"mappings":"AAsCA,OAAOA,UAAS,MAAO,wBAAwB;AAC/C,eAAe;EACXC,IAAI,EAAE,aAAa;EACnBC,IAAIA,CAAA,EAAG;IACH,OAAO;MACHD,IAAI,EAAE,IAAI;MACVE,KAAK,EAAE,IAAI;MACXC,IAAI,EAAE,IAAI;MACVC,SAAS,EAAE,IAAI;MACfC,MAAM,EAAE,IAAI;MACZC,OAAO,EAAE,IAAI;MACbC,MAAM,EAAE,IAAI;MACZC,WAAW,EAAE,EAAE;MACfC,MAAM,EAAE,YAAY;MACpBC,GAAG,EAAE;IACT;EACJ,CAAC;EACDC,OAAO,EAAE;IACL,MAAMC,cAAcA,CAAA,EAAE;MAClB,MAAMC,GAAE,GAAI,MAAMC,KAAK,CAAC,mCAAmC,CAAC;MAC5D,MAAMb,IAAG,GAAI,MAAMY,GAAG,CAACE,IAAI,CAAC,CAAC;MAE7B,IAAI,CAACb,KAAI,GAAID,IAAI,CAACC,KAAK;MACvB,IAAI,CAACC,IAAG,GAAIF,IAAI,CAACE,IAAI;MACrB,IAAI,CAACC,SAAQ,GAAIH,IAAI,CAACG,SAAS;IACnC,CAAC;IACD,MAAMY,cAAcA,CAACC,CAAC,EAA